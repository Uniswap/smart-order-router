{
  "name": "@uniswap/smart-order-router",
  "version": "1.0.1",
  "description": "Uniswap Smart Order Router",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/Uniswap/uniswap-smart-order-router",
  "license": "MIT",
  "keywords": [],
  "bin": {
    "uniswap-smart-order-router": "./bin/run"
  },
  "scripts": {
    "compile-v3-types": "npx typechain --target ethers-v5 --out-dir src/types/v3 './node_modules/@uniswap/?(v3-core|v3-periphery)/artifacts/contracts/**/*.json'",
    "compile-external-types": "npx typechain --target ethers-v5 --out-dir src/types/other 'src/abis/**/*.json'",
    "build": "run-p compile-v3-types compile-external-types && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "version": "standard-version",
    "prepare-release": "run-s cov:check doc:html version doc:publish"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@bitauth/libauth": "^1.17.1",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.2",
    "@typechain/ethers-v5": "^7.0.0",
    "@types/bunyan": "^1.8.6",
    "@types/lodash": "^4.14.168",
    "@uniswap/default-token-list": "^2.0.0",
    "@uniswap/sdk-core": "^3.0.0-alpha.1",
    "@uniswap/token-lists": "*",
    "@uniswap/v3-core": "^1.0.0",
    "@uniswap/v3-sdk": "^3.0.0-alpha.0",
    "axios": "^0.21.1",
    "bunyan": "^1.8.15",
    "bunyan-debug-stream": "^2.0.0",
    "cli-logger": "^0.5.40",
    "ethers": "^5.1.4",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "jsbi": "^3.1.4",
    "lodash": "^4.17.21",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^6.0.1",
    "prettier": "^2.1.1",
    "standard-version": "^9.0.0",
    "ts-node": "^9.0.0",
    "typechain": "^5.0.0",
    "typescript": "^4.2.2"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "prettier": {
    "singleQuote": true
  }
}
